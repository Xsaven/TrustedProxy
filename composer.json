{
    "name": "bfg/proxy",
    "description": "Set trusted proxies for Laravel",
    "keywords": ["proxy", "trusted proxy", "load balancing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Xsaven",
            "email": "gms.sw.xsaven@gmail.com"
        },
        {
            "name": "Chris Fidao",
            "email": "fideloper@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/contracts": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "illuminate/http": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.5.8|^9.3.3|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Bfg\\Proxy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bfg\\Proxy\\TrustedProxyServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
